package space1;

class Location {
	private int x;
	private int y;
	public Location(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public int getX() { return x; }
	public int getY() { return y; }
}
interface SizeCompare {
	boolean leftGreaterThanRight(Location left, Location Right);
}

